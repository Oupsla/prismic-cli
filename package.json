{
  "author": "prismic.io developers",
  "license": "Apache-2.0",
  "name": "prismic-cli",
  "version": "3.3.2",
  "description": "Command line to bootstrap prismic projects.",
  "scripts": {
    "build": "babel ./lib/ -d ./bin/",
    "watch": "npm run prebuild; nodemon --watch ./lib --exec \"npm run lint; npm run build\"",
    "prebuild": "mkdir -p ./bin",
    "prepublish": "npm run prebuild; npm run build;",
    "lint": "eslint lib/**/*.js",
    "test": "tape test/run.js"
  },
  "bin": {
    "prismic": "bin/prismic.js"
  },
  "preferGlobal": true,
  "dependencies": {
    "adm-zip": "~0.4.7",
    "babel-runtime": "~6.23.0",
    "command-line-commands": "~2.0.0",
    "command-line-usage": "~4.0.0",
    "inquirer": "~3.0.6",
    "lodash": "~4.17.4",
    "prismic.io": "~3.5.5",
    "request": "~2.81.0",
    "shelljs": "~0.7.7",
    "tmp": "~0.0.31"
  },
  "devDependencies": {
    "babel-cli": "~6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "~7.2.1",
    "babel-plugin-syntax-async-functions": "~6.13.0",
    "babel-plugin-transform-regenerator": "~6.22.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.13.0",
    "babel-preset-es2015": "~6.24.0",
    "babel-watch": "~2.0.6",
    "eslint": "~3.18.0",
    "eslint-config-airbnb-base": "~11.1.0",
    "eslint-plugin-import": "~2.2.0",
    "tape": "~4.6.3",
    "tape-spawn": "~1.4.2"
  },
  "engines": {
    "node": ">=4.8.1",
    "npm": ">=2.15.11"
  }
}
