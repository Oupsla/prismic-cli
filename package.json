{
  "name": "prismic-cli",
  "description": "Command line to bootstrap prismic projects.",
  "version": "3.7.0-beta.5",
  "author": "prismic.io developers",
  "license": "Apache-2.0",
  "scripts": {
    "build": "babel ./lib/ -d ./bin/ --copy-files",
    "watch": "npm run prebuild; nodemon --watch ./lib --exec \"npm run lint:fix; npm run build\"",
    "prebuild": "mkdir -p ./bin",
    "prepublish": "npm run prebuild; npm run build;",
    "lint": "eslint lib/**/*.js",
    "lint:fix": "eslint lib/**/*.js --fix",
    "test": "tape test/run.js"
  },
  "bin": {
    "prismic": "bin/prismic.js"
  },
  "preferGlobal": true,
  "dependencies": {
    "@sentry/node": "^5.11.1",
    "adm-zip": "^0.4.13",
    "babel-runtime": "~6.26.0",
    "command-line-commands": "~2.0.1",
    "command-line-usage": "~5.0.4",
    "deep-equal": "^1.1.0",
    "inquirer": "~5.2.0",
    "mustache": "^3.1.0",
    "open": "^7.0.0",
    "prismic.io": "~3.6.1",
    "ramda": "^0.26.1",
    "request": "~2.85.0",
    "shelljs": "~0.8.1",
    "tmp": "~0.0.33",
    "vue-docgen-api": "^3.25.0"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.2.2",
    "babel-plugin-syntax-async-functions": "~6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "~6.26.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-watch": "~2.0.7",
    "eslint": "~4.19.1",
    "eslint-config-airbnb-base": "~12.1.0",
    "eslint-plugin-import": "~2.10.0",
    "tape": "~4.9.0",
    "tape-spawn": "~1.4.2"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 3.0.0"
  }
}
